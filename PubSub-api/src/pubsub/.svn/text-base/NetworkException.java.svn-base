package pubsub;

/**
 *
 * @author tsilo
 */
public class NetworkException extends Exception {

    private static final long serialVersionUID = 1L;

    public NetworkException(String string) {
        super(string);
    }

    public NetworkException(Throwable t) {
        super(t);
    }

    public NetworkException(String mesg, Throwable t) {
        super(mesg, t);
    }
}
